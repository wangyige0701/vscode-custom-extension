{
	"extends": "./tsconfig.basic.json",
	"compilerOptions": {
		"typeRoots": ["./node_modules/@types", "./src/@types/*.d.ts"],
		"paths": {
			"@/*": ["src/*"],
			"@app/*": ["src/app/*"],
			"@background/*": ["src/app/background/*"],
			"@time/*": ["src/app/time/*"]
		}
	},
	"exclude": [
		"src/uninstall.ts",
		"src/library/external/**/*"
	],
	"include": ["src/**/*"]
}
